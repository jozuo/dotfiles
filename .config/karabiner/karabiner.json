                "conditions": [
                  {
                    "bundle_identifiers": [
                      "^com\\.vmware\\.fusion",
                      "^com\\.parallels\\.desktop\\.console"
                    ],
                    "file_paths": [
                      "/Applications/VMware Fusion\\.app/Contents/MacOS/VMware Fusion",
                      "/Applications/Parallels Desktop\\.app/Contents/MacOS/prl_client_app"
                    ],
                    "type": "frontmost_application_if"
